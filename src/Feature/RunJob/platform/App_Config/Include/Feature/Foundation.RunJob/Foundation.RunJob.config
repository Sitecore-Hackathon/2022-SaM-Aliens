<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
        <sitecore>
            <pipelines>
                <initialize>
                    <processor type="Foundation.RunJob.Platform.RegisterRoutes, Foundation.RunJob.Platform" patch:before="processor[@type='Sitecore.Mvc.Pipelines.Loader.InitializeRoutes, Sitecore.Mvc']" />
                </initialize>
            </pipelines>

            <services>
                <register serviceType="Sitecore.Abstractions.BaseJobManager, Sitecore.Kernel" implementationType="Foundation.RunJob.Platform.OverridenJobManager, Foundation.RunJob.Platform" />
            </services>
        </sitecore>
</configuration>